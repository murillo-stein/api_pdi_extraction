    <script>
        $.extend($.fn.dataTable.defaults, {
            language: {
                "url": "/assets/translate/datatable/pt_BR.json"
            }
        });

        $(document).ready(function () {
            $("body").on("click", ".start-import", function (e) {
                e.preventDefault();
            });

            function initSideMenuEvent() {
                let extraSmallDeviscesWidth = 768;

                function callback() {
                    let fdzMain = $("#fdz-main");
                    let windowWidth = $(window).width();

                    fdzMain.removeClass('fdz-main-side-menu');
                    if (windowWidth <= extraSmallDeviscesWidth) {
                        fdzMain.addClass('fdz-main-side-menu');
                    }
                }

                // initialize side menu if appropriate
                callback();

                $(window).resize(callback);
            }

            setTimeout(function () {
                $('[data-toggle="popover"]').each(function (e, elem) {
                    let _this = this;
                    let trigger = $(_this).attr('data-trigger');
                    let popover = $(_this).popover({
                        boundary: 'window',
                        container: 'body',
                        html: true,
                        title: $(this).data('popover-title'),
                        content: $(this).data('popover-content'),
                        trigger: trigger,
                        animation: false
                    });

                    if (trigger == 'click') {
                        popover.on("show.bs.popover", function (e) {
                            $("[rel=popover]").not(e.target).popover("destroy");
                            $(".popover").remove();
                        });
                    }

                    if (trigger == 'hover') {
                        popover.on("mouseenter", function () {
                            $(_this).popover("show");

                            $(_this).siblings('[data-toggle="popover"]').each(function () {
                                $(this).popover("hide");
                            });
                        }).parent().on("mouseleave", function () {
                            $(_this).popover("hide");
                        });
                    }
                });
            }, 500)

            function initMenuHoverLogic() {
                var path = window.location.pathname;

                if (path == '/plano-de-desenvolvimento/dashboard' || path == '/feedbacks/historico' || path == '/feedbacks/gestao' || path == '/gestao/1on1') {
                    result = path.replace(/\//g, '-');
                    //console.log(result)
                    $('#fdz-menu .navbar-inverse .fdz-menu-active-item').removeClass('fdz-menu-active-item');
                    $('#fdz-menu .fdz-menu-nav' + result).addClass('fdz-menu-active-item');
                    return;
                }

                var regex = /(\/\w+)/i;
                var result = path.match(regex);
                result = result[0].replace('/', '');

                $('#fdz-menu .navbar-inverse .fdz-menu-active-item').removeClass('fdz-menu-active-item');
                $('#fdz-menu .fdz-menu-nav-' + result).addClass('fdz-menu-active-item');
            }

            function initBookmarkDelete() {
                $(".bookmark-delete").on("click", function () {
                    var bookmarkId = $(this).attr('data-id');

                    swal({
                        title: "Você tem certeza?",
                        text: "Este favorito não poderá ser recuperado",
                        type: "warning",
                        showCancelButton: true,
                        confirmButtonColor: "#DD6B55",
                        confirmButtonText: "Sim",
                        cancelButtonText: "Cancelar",
                        closeOnConfirm: false,
                        closeOnCancel: true
                    }, function (isConfirm) {
                        if (!isConfirm) {
                            return;
                        }

                        $.ajax({
                            type: 'DELETE',
                            url: `/api/objetivos/favoritos/${bookmarkId}`,
                            success: function (data) {
                                swal("Excluido!", "Favorito deletado com sucesso", "success");
                                location.reload();
                            },
                            error: function (data) {
                                swal("Oops!", "Não é possível deletar o favorito", "error");
                            }
                        });
                    });
                });
            }

            function initBookmarkGlobalDelete() {
                $(".bookmarkGlobal-delete").on("click", function () {
                    var bookmarkId = $(this).attr('data-id');

                    swal({
                        title: "Você tem certeza?",
                        text: "Se você excluir esse filtro, todos os colaboradores perderão acesso ao filtro. Deseja excluir mesmo assim?",
                        type: "warning",
                        showCancelButton: true,
                        confirmButtonColor: "#DD6B55",
                        confirmButtonText: "Sim",
                        cancelButtonText: "Cancelar",
                        closeOnConfirm: false,
                        closeOnCancel: true
                    }, function (isConfirm) {
                        if (!isConfirm) {
                            return;
                        }

                        $.ajax({
                            type: 'DELETE',
                            url: `/api/objetivos/favoritos/${bookmarkId}`,
                            success: function (data) {
                                swal("Excluido!", "Favorito deletado com sucesso", "success");
                                location.reload();
                            },
                            error: function (data) {
                                swal("Oops!", "Não é possível deletar o favorito", "error");
                            }
                        });
                    });
                });
            }

            function initNotificationMarkAsRead() {
                $(".mark-as-read").on("click", function () {
                    $.ajax({
                        type: 'GET',
                        url: '/notification/marcar-como-lidas',
                        success: function (data) {
                            location.reload();
                        },
                        error: function (data) {
                            swal("Oops!", "Falha ao limpar notificações", "error");
                        }
                    });
                });
            }

            initSideMenuEvent();
            initMenuHoverLogic();
            initBookmarkDelete();
            initBookmarkGlobalDelete();
            initNotificationMarkAsRead();
        });

        $(document).on('click', "#id-activate-support", function () {
            swal({
                title: "Liberar suporte",
                text: "Ao ativar o suporte, o time de Sucesso do Cliente da Feedz terá acesso aos dados da empresa por 48h." +
                    "Você deseja ativar o suporte?",
                type: "warning",
                showCancelButton: true,
                confirmButtonColor: "#DD6B55",
                confirmButtonText: "Sim, liberar suporte",
                cancelButtonText: "Cancelar",
                closeOnConfirm: false,
                closeOnCancel: true
            }, function (isConfirm) {
                $.blockUI();
                if (isConfirm) {

                    $.ajax({
                        type: 'POST',
                        url: '/empresa/liberar-suporte',
                        success: function (data) {
                            $.unblockUI();
                            if (data.success == true) {
                                swal("Sucesso!", "Suporte ativado com sucesso!", "success");
                            } else {
                                swal("Oops", data.message, "error");
                            }
                        },
                        error: function (data) {
                            $.unblockUI();
                            swal("Oops!", "Ocorreu um erro. Por favor tente novamente", "error");
                        }
                    });
                } else {
                    $.unblockUI();
                }
            });
        });

        $(document).on("click", function (event) {
            let $trigger = $(".fdz-dropdown");
            let isModalButton = $(event.target).hasClass('fdz-dropdown-btn') || $(event.target).parents('.fdz-dropdown-btn').hasClass('fdz-dropdown-btn')

            if ((!isModalButton && $trigger.length) && ($trigger !== event.target && !$trigger.has(event.target).length)) {
                $(".fdz-dropdown").trigger('remove').remove()
            }
        });

