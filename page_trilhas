    <script>
        const openedTrail = document.getElementById("opened_trail");
        const rightContent = document.getElementById("right_menu");
        const mainContent = document.getElementById("main_content");
        const createBtn = document.getElementById("create_new_btn");
        openedTrail.style.display = "none";

        let trail = null;
        if (createBtn != null) {
            createBtn.addEventListener('click', () => {
                window.location.href = "/trilhas/232683/trilha/0/editar/1";
            })
        }

        function closeModals() {
            $('.fdz-new-modal').fadeOut(500);
            $('#modals').html('');
            $(".modal-backdrop.in").remove();

            const body = $("body");

            if (body.hasClass("modal-open")) {
                body.removeClass("modal-open");
            }
        }

        function getAllRows() {
            return $('.trail-row');
        }

        function getRows(completed, expired) {
            const $rows = $(`.trail-row`).filter(function () {
                if (expired) {
                    return ($(this).data('type') == "1" && $(this).data('status') == "expired");
                }
                if (completed) {
                    return $(this).data('status') == "completed";
                }
                return ($(this).data('status') != "completed" && ($(this).data('type') == "2" || $(this).data('type') == "3")) || ($(this).data('type') == "1" && $(this).data('status') != "expired" && $(this).data('status') != "completed");
            });

            let label = "";
            if (completed) {
                label = "finalizado";
            } else {
                label = "ativo";
            }

            if ($rows.length == 0) {
                if ($('.fdz-empty-content').length == 0) {
                    $('#main_content').append(`<div class="fdz-empty-content larger"><img class="fdz-empty-content-img" src="/assets/images/trails/empty_state_list.png"/><p class="fdz-empty-content-label larger"><b>Nenhum</b> plano ${label} ativo no momento</p></div>`);
                } else {
                    $('.fdz-empty-content').remove();
                    $('#main_content').append(`<div class="fdz-empty-content larger"><img class="fdz-empty-content-img" src="/assets/images/trails/empty_state_list.png"/><p class="fdz-empty-content-label larger"><b>Nenhum</b> plano ${label} ativo no momento</p></div>`);
                }
            }
            else if ($rows.length == 1) {
                let trailId = Number(localStorage.getItem('taskOpened-trailId'));
                $('.fdz-empty-content').remove();
                if (trailId == 0) {
                    let id = Number($($rows[0]).data('id'));
                    trail = new Trail(id);
                }
            } else {
                $('.fdz-empty-content').remove();
            }
            return $rows;
        }

        function loadTask() {
            let trailId = Number(localStorage.getItem('taskOpened-trailId'));
            if (trailId != 0) {
                let progress = Number($(`li[data-id=${trailId}]`).data('progress'));
                if (progress < 100) {
                    trail = new Trail(trailId);
                }
            }
        }

        function stopVideo() {
            let iframes = $('iframe');
            let videos = $('video');
            if (iframes) {
                iframes.each(function (index, iframe) {
                    $(iframe).attr('allow', 'accelerometer; clipboard-write; encrypted-media; gyroscope; picture-in-picture');
                    $(iframe).attr('src', $(iframe).attr('src'));
                });
            }
            if (videos) {
                videos.each(function (index, video) {
                    $(video).attr('autoplay', false);
                    $(video).pause();
                });
            }
        };

        $(document).on('click', '.note-btn.btn.btn-default.btn-sm', () => {
            $(".modal-backdrop.in").remove();
        });

        window.addEventListener('DOMContentLoaded', () => {
            let completed = false;
            const closeModal = $('.fdz-modal-close');
            const activeTrails = $('#active-trails');
            const completedTrails = $('#completed-trails');
            const expiredTrails = $('#expired-trails');

