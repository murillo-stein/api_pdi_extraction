    <script>
        $(document).ready(function () {
            $.datepicker.setDefaults($.datepicker.regional['pt-BR']);

            const language = 'pt_BR';

            switch (language) {
                case 'es_ES':
                    var lg = 'es';
                    break;
                case 'pt_BR':
                    var lg = 'pt-BR';
                    break;
                case 'en_US':
                    var lg = 'en-US';
                    break;
                default:
                    var lg = 'pt-BR';
                    break;
            }

            $.datepicker.setDefaults($.datepicker.regional[lg]);
        });
    </script>
