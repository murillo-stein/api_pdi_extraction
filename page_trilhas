            });

            $('#menu_trail_status_active').on('click', function (e) {
                const btn = $('#menu_trail_status_active');
                const otherBtn = $('#menu_trail_status_completed');
                const expired = $('#menu_trail_status_expired');
                if (!(btn.hasClass("active"))) {
                    btn.addClass("active");
                    otherBtn.removeClass("active");
                    expired.removeClass("active");
                }
                completed = false;
                getAllRows().hide();
                getRows(completed, false).show();
                $('#opened_trail').html("");
                $('#opened_trail_modal').html("");
                $("#modals").html("");

                $('#opened_trail').hide();
                $('#opened_trail_modal').hide();
                $('#trails_list').show();

                $('#clear-fdz-filter-employees').trigger('click');
            });

            $('#menu_trail_status_completed').on('click', function (e) {
                const btn = $('#menu_trail_status_completed');
                const otherBtn = $('#menu_trail_status_active');
                const expired = $('#menu_trail_status_expired');
                if (!(btn.hasClass("active"))) {
                    btn.addClass("active");
                    otherBtn.removeClass("active");
                    expired.removeClass("active");
                }
                completed = true;
                getAllRows().hide();
                getRows(completed, false).show();
                $('#opened_trail').html("");
                $('#opened_trail_modal').html("");
                $("#modals").html("");

                $('#opened_trail').hide();
                $('#opened_trail_modal').hide();
                $('#trails_list').show();

                $('#clear-fdz-filter-employees').trigger('click');
            });

            $('#menu_trail_status_expired').on('click', function (e) {
                const finished = $('#menu_trail_status_completed');
                const active = $('#menu_trail_status_active');
                const expired = $('#menu_trail_status_expired');

                active.removeClass("active");
                finished.removeClass("active");
                expired.addClass("active");

                completed = true;
                getAllRows().hide();
                getRows(completed, true).show();
                $('#opened_trail').html("");
                $('#opened_trail_modal').html("");
                $("#modals").html("");

                $('#opened_trail').hide();
                $('#opened_trail_modal').hide();
                $('#trails_list').show();

                $('#clear-fdz-filter-employees').trigger('click');
            });

            $('body').on('click', '.trail-back-btn', function (e) {
                $('#opened_trail').html("");
                $('#opened_trail_modal').html("");
                $("#modals").html("");

                $('#opened_trail').hide();
                $('#opened_trail_modal').hide();
                $('#trails_list').show();
            });

            $('.trail-row').on('click', function (e) {

                const id = $(this).data("id");
                trail = new Trail(id);

                $('#opened_trail').html('');
            });

            $('.fdz-profile-group-task').on('click', function (e) {
                $('#tasks_modal').show();
            });

            $('.fdz-btn-okr').on('click', function (e) {
                const okr = $(this).data('okr');
                const groupOkr = $(`.fdz-trails-krs[data-okr="${okr}"]`)
                if ($(this).hasClass('opened')) {
                    groupOkr.hide();
                } else {
                    groupOkr.show();
                }
                $(this).toggleClass('opened');
                $(this).toggleClass('closed');
            });

            $('.see-profile').on('click', function (e) {
                e.preventDefault();
                const employeeId = $(this).data('employee');
                window.open(`/colaboradores/${employeeId}/perfil`, '_blank')
            });

            $('body').on('click', '.trail-open-btn', function (e) {
                const group = $(this).data('group');
                const groupTasks = $(`.fdz-profile-trail-group-tasks[data-group="${group}"]`)
                if ($(this).hasClass('opened')) {
                    groupTasks.hide();
                } else {
                    groupTasks.show();
                }
                $(this).toggleClass('opened');
                $(this).toggleClass('closed');
            });

            $('body').on('click', '.fdz-modal-actions a.back-btn', function (e) {
                closeModals();
                $('#tasks_modal').hide();
            })

            $('body').on('click', '.fdz-profile-group-task', function (e) {
                const taskId = $(this).data("task");
                const groupId = $(this).data("group");
                trail.mountSingleTask(taskId, groupId);
            });

            $('body').on('click', function (e) {
                if ($(e.target).is('.fdz-new-modal')) {
                    closeModals();
                }
            });

            $('body').on('keyup change', '.task-status', function (e) {
                const taskId = $(this).data("task");
                let percentValue = $(this).val();
                let maxValue = 100;

                if (percentValue == "") {
                    percentValue = 0;
                } else if (percentValue > maxValue) {
                    percentValue = maxValue;
                    e.target.value = percentValue;
                }

                $(`.fdz-progress[data-task="${taskId}"]`).css('width', `${percentValue}%`);
            });


            $('body').on('click', '.fdz-modal-close', function (e) {
                closeModals();
            });

            $('body').on('submit', '.fdz-form-task', function (e) {
                e.preventDefault();
                const taskId = $(this).data("task");
                const groupId = $(this).data("group");
                const trailId = $(this).data("trail");

                const formData = $(this).serializeArray();
                const url = `/trilhas/232683/trilha/${trailId}/grupos/${groupId}/tarefas/${taskId}`;

                $(`tasks_modal_${taskId}`).fadeOut(500);
                $.blockUI();
                sendTaskUpdate(url, formData, groupId, trailId);
            });

            $('body').on('click', '.delete-trail.delete-btn', function (e) {
                askConfirmation();
            });

            $('body').on('click', '.finish-trail-btn', function (e) {
                swal({
                    title: "Finalizar",
                    text: "Você tem certeza disso?",
                    type: "warning",
                    showCancelButton: true,
                    confirmButtonText: "Finalizar",
                    cancelButtonText: "Cancelar",
                    closeOnConfirm: true
                }, function (isConfirm) {
                    if (!isConfirm) {
                        return;
                    }

                    trail.sendFinishTrail().then((res) => {
                        if (res.success) {
                            swal({
                                type: 'success',
                                title: "Opa!",
                                text: "Finalizado com sucesso!"
                            }, function () {
                                location.reload();
                            });
                        } else {
                            swal({
                                type: 'error',
                                title: "Opa!",
                                text: res.message
                            });
                        }
                    });
                });

            });

            loadTask();

        });

        closeModals();

        async function sendTaskUpdate(url, data, groupId, trailId) {
            function onSuccess() {
                swal({
                    type: 'success',
                    title: "Opa!",
                    text: "Salvo com sucesso!"
                }, function () {
                    if (Number(data[0].value) < 100) {
                        localStorage.setItem('taskOpened-groupId', groupId);
                        localStorage.setItem('taskOpened-trailId', trailId);
                    }
                    location.reload();
                }
                );
            }

            function onError(message) {
                swal({
                    type: 'error',
                    title: "Opa!",
                    text: message
                });
            }

            $.post(url, data).done((res) => {
                if (res.success) {
                    onSuccess();
                } else {
                    onError(res.message);
                }
            }).fail((res) => {
                onError(res.responseJSON.message);
            }).always($.unblockUI);
        }

        function askConfirmation(trailDel, group) {

            swal({
                title: "Deletar",
                text: "Você tem certeza disso?",
                type: "warning",
                showCancelButton: true,
                confirmButtonText: "Deletar",
                cancelButtonText: "Cancelar",
                closeOnConfirm: true
            }, function (isConfirm) {
                if (!isConfirm) {
                    return;
                }

                const url = (window.location.pathname).split("/editar")[0];
                $.ajax({
                    url: `${url}`,
                    type: 'DELETE'
                }).then((res) => {
                    if (!res.success) {
                        swal({
                            type: 'error',
                            title: `Opa!`,
                            text: `Algo de errado não está certo!`
                        });
                    } else {
                        window.location.href = "/trilhas"
                    }
                });
                return;
            });
        }

        class Trail {

            constructor(id) {
                this.id = id;
                this.trail = null
                this.initEvents();
            }

            initEvents() {
                const _this = this;
                $.blockUI();

                function onSuccess(result) {
                    if (result.success && result.trail) {
                        const trail = result.trail;
                        _this.trail = trail;
                        $('#opened_trail').append(_this.mount(trail));
                        $('#opened_trail_modal').append(_this.mount(trail));

                        const individualTrailInfo = $('.fdz-people-trail-info');
                        $('.fdz-profile-trail-group-tasks').hide();
                        $('.trail-open-btn').removeClass('opened');
                        $('.trail-open-btn').addClass('closed');

                        $('#trails_list').hide();
                        $('#opened_trail').show();
                        $('#opened_trail_modal').show();

                        let groupId = Number(localStorage.getItem('taskOpened-groupId'));
                        if (groupId != 0) {
                            $(`button[data-group=${groupId}]`).trigger("click");
                            localStorage.removeItem('taskOpened-groupId');
                            localStorage.removeItem('taskOpened-trailId');
                            $.unblockUI();
                        }
                        $.unblockUI();
                    }
                    else {
                        onError();
                    }
                }

                function onError() {
                    $.unblockUI();
                    swal({
                        type: 'error',
                        title: "Opa!",
                        text: "Não foi possível realizar a operação. Por favor, tente novamente."
                    })
                }

                _this.getTrailDetails(_this.id, onSuccess, onError);
            }

            mount(trail) {
                const _this = this;
                return `${_this.mountCloneButton(trail)}
<div class="fdz-people-trail-info ${_this.pickTrailClass()}">
    <div class="fdz-people-trail-header ${_this.pickTrailClass()}">
        ${_this.mountBackButton(trail)}
        ${_this.mountTrailPhoto(trail)}
        <div class="fdz-people-trail-header-info ${_this.pickTrailClass()} ${_this.employeeTrailClass(trail)}">
            <div class="top-info">
                <div class="trail-info">
                    ${_this.mountTitle(trail)}
                    ${_this.mountType(trail)}
                    ${_this.mountJobDepartmentInfo(trail)}
                </div>
                ${_this.mountManagerInfo(trail)}
            </div>
            <div class="trail-data">
                <p class="fdz-paragraph">
                    ${_this.mountDtStart(trail)}
                </p>
                ${_this.mountDtFinish(trail)}
            </div>
            <div class="fdz-progress-bar-wrapper">
                ${_this.mountMainProgressBar(trail)}
            </div>
        </div>
        ${_this.mountEditOrDeleteBtn(trail)}
    </div>
    <div class="fdz-profile-trail-groups-tasks">
        ${_this.mountGroups(trail.groups, trail)}
    </div>
    ${_this.mountFinishButton(trail)}
    
</div>`;
            }

            mountManagerInfo(trail) {
                if (trail.profile.direct_manager != null && trail.profile.direct_manager != "undefined" && location.pathname.indexOf("/trilhas/gestao") > -1) {
                    return `<div class="manager-info">
                    <img src="${trail.profile.direct_manager.photo}" class="fdz-m-right-8" />
                    <p class="fdz-paragraph fdz-m-0">
                        ${trail.profile.direct_manager.name}
                    </p>
                </div>`;
                }
                return "";
            }

            mountTrailPhoto(trail) {

                let photoClass = "fdz-trail-header-photo";
                if (location.pathname.indexOf("/trilhas/gestao") > -1) {
                    return `<img class="${photoClass}" src="${trail.profile.photo}">`;
                }

                let picture = "";
                return `<img class="fdz-trail-header-photo employee" src="${picture}">`;
            }

            mountEditOrDeleteBtn(trail) {
                if (!this.pickTrail) {
                    if (!trail.is_finished && trail.status != 100 && trail.type == "1") {
                        return `
                        <a href="/trilhas/232683/trilha/${trail.id}/editar/${trail.type}" class="fdz-people-trail-btn right trail-edit-btn">
                            <img src="/assets/images/trails/edit.svg" width="24" height="24"/>
                        </a>
                    `;
                    }
                }
                return ``;
            }

            mountDtStart(trail) {
                if (!this.pickTrail) {
                    return `Inicio em ${moment(trail.dt_start).format("DD/MM/YY")}`;
                }
                return "";
            }

            mountDtFinish(trail) {
                if (!this.pickTrail) {
                    if (trail.type == 2 && location.pathname == "/trilhas") {
                        return "";
                    }
                    return `
                    <p class="fdz-paragraph">
                        Previsto ${moment(trail.dt_finish).format("DD/MM/YY")}
                    </p>
                    `;
                }
                return "";
            }

            mountMainProgressBar(trail) {
                if (!this.pickTrail) {
                    let progressClass = '';
                    if (trail.status <= 30) {
                        progressClass = 'danger';
                    }
                    else if (trail.status > 30 && trail.status <= 60) {
                        progressClass = 'warning';
                    } else {
                        progressClass = 'good';
                    }

                    return `
                        <div class="fdz-progress-bar">
                            <div class="fdz-progress ${progressClass}" style="width: ${trail.status}%;"></div>
                        </div>
                        <div class="fdz-progress-bar-info">
                            <p class="fdz-paragraph ${progressClass} bold">
                                ${trail.status}%
                            </p>
                            <p class="fdz-paragraph">
                                <b class="${progressClass}">
                                    ${trail.task_number.done}
                                </b>&nbsp;
                                de ${trail.task_number.total} tarefas                            </p>
                        </div>`;
                }
                return "";
            }

            employeeTrailClass(trail) {
                if (!trail.is_finished && trail.status != 100) {
                    return "employee-trail";
                }
                return "employee-trail-finished";
            }

            pickTrailClass() {
                if (this.pickTrail) {
                    return "modal-pick-trail";
                }
                return "";
            }

            mountBackButton(trail) {
                if (!this.pickTrail) {
                    return ` <button class="fdz-people-trail-btn left trail-back-btn">
                            <img src="/assets/images/trails/back.svg" width="24" height="24"/>
                        </button>`;
                }
                return "";
            }

            mountTitle(trail) {
                if (trail.type == 1) {
                    return `
                <h3 class="fdz-title">
                    ${trail.name}
                </h3>
                `;
                }
                if (trail.type == 2 && location.pathname == "/trilhas") {
                    return `
                <h3 class="fdz-title">
                    Onboarding de ${trail.profile.name}
                </h3>
                `;
                }
                return `
            <h3 class="fdz-title">
                ${trail.profile.name}
            </h3>
            `;
            }

            mountType(trail) {
                if (trail.type == 1) {
                    return `
                <p class="fdz-paragraph fdz-m-0">
                    ${trail.pdi_type.title}
                </p>
                `;
                }
                return "";
            }

            mountJobDepartmentInfo(trail) {
                if (trail.profileInfo[0].job != null && trail.profileInfo[0].job != "undefined") {
                    return ` <p class="fdz-paragraph fdz-m-bottom-0">
                        ${trail.profileInfo[0].job.title}
                        <p class="fdz-paragraph">
                            ${trail.profileInfo[0].department.name}
                        </p>
                    </p>`;
                }

                return "";
            }

            mountFinishButton(trail) {
                return "";
            }

            mountCloneButton(trail) {
                return "";
            }

            mountGroups(groups, trail) {
                const _this = this;
                let groupHTML = '';
                groups.forEach(group => {
                    groupHTML += `<div class="fdz-profile-trail-group">
    <div class="fdz-profile-trail-group-header fdz-trail-withouth-progress">
        <div class="trail-group-info">
            <h3 class="fdz-title">
                ${group.name}
            </h3>
            <p class="fdz-paragraph">
                ${group.description}
            </p>
            ${_this.mountGroupProgressBar(group)}
        </div>
        <button class="fdz-people-trail-btn trail-open-btn opened" data-group="${group.id}">
            <img src="/assets/images/trails/back.svg" width="24" height="24"/>
        </button>
    </div>
    <div class="fdz-profile-trail-group-tasks" data-group="${group.id}">
        ${_this.mountTasks(group.tasks, group, trail)}
    </div>
</div>`
                });
                return groupHTML;
            }

            mountGroupProgressBar(group) {
                if (!this.pickTrail) {
                    return `
                <div class="fdz-progress-bar-wrapper">
                    <div class="fdz-progress-bar">
                        <div class="fdz-progress" style="width: ${group.status}%;"></div>
                    </div>
                    <div class="fdz-progress-bar-info">
                        <p class="fdz-paragraph bold blue">
                            ${group.status}%
                        </p>
                        <p class="fdz-paragraph">
                            <b class="blue">
                                ${group.task_number.done}
                            </b>&nbsp;
                            de ${group.task_number.total} tarefas                        </p>
                    </div>
                </div>`;
                }
                return "";
            }

            mountTasks(tasks, group, trail) {
                const _this = this;
                let taskHTML = '';
                tasks.forEach(task => {
                    taskHTML += `<div class="fdz-profile-group-task fdz-m-bottom-8  ${task.status == 100 && !this.pickTrail ? 'completed' : ''}" data-task="${task.id}" data-group="${group.id}">
    <div class="fdz-task-step fdz-m-right-8">
        <p class="fdz-paragraph ${task.status == 100 && !this.pickTrail ? 'good' : 'blue'} fdz-m-0">
            ${_this.mountTaskStep(task.step)}
        </p>
    </div>
    <div class="fdz-task-information">
        <h5 class="fdz-title ${task.status == 100 && !this.pickTrail ? 'stroked ' : ''} fdz-m-0 fdz-full fdz-break-word">
            ${task.name}
        </h5>
        <p class="fdz-paragraph ${task.status == 100 && !this.pickTrail ? 'good' : 'fdz-d-none'} fdz-m-right-8 fdz-m-bottom-0">
            ${moment(task.updated_at).format('DD/MM/YY HH:mm')}
        </p>
        ${!this.pickTrail ? _this.mountTaskImage(task) : _this.mountTaskIcon(task.type)}        
    </div>
</div>`;
                });
                return taskHTML;
            }

            mountSingleTask(taskID, groupID) {
                const _this = this;
                const group = (_this.trail.groups).find((group) => group.id == groupID);
                const task = (group.tasks).find((task) => task.id == taskID);

                _this.mountTaskModal(task, group);
            }


            mountTaskImage(task) {

                let isCompleted = task.status == 100 ? "good" : "blue";
                if (task.status == 100) {
                    return `<div class="fdz-circular-progress-container">
                        <div class="fdz-circular-progress-wrapper">
                            <div class="fdz-circular-progress-box">
                                <div class="fdz-flex-row bottom-info trail-info">
                                    <img src="/assets/images/trails/task_completed.svg" />
                                </div>


                            </div>
                        </div>
                    </div>`;
                } else {
                    return `<div class="fdz-circular-progress-container">
                        <div class="fdz-circular-progress-wrapper">
                            <div class="fdz-circular-progress-box">
                                <div class="fdz-circular-progress-percent">
                                    <svg>
                                        <circle cx="18" cy="18" r="18"></circle>
                                        <circle cx="18" cy="18" r="18" stroke-dasharray="${task.status * 114 / 100}, 114"></circle>
                                    </svg>
                                </div>

                                <div class="fdz-circular-progress-number">
                                    <p class="fdz-paragraph">
                                        ${task.status}%
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>`;
                }

            }

            mountTaskIcon(taskType) {
                return `<img class="trail-progress fdz-m-right-8" src="/assets/images/trails/groups/type_${taskType}.svg" /> `
            }

            mountTaskModal(task, group) {
                const _this = this;
                const trail = this.trail;
                $("#modals").append(`<div class="fdz-new-modal" id="tasks_modal_${task.id}">
    <div class="fdz-modal-content fdz-trail-info-modal custom-scrollbar" style="width: 1000px;">
        <div class="fdz-modal-header">
            <div class="fdz-flex-align-center">
                <div class="fdz-step fdz-m-right-8">
                    <p class="fdz-paragraph blue fdz-m-0">
                        ${_this.mountTaskStep(task.step)}
                    </p>
                </div>
                <h3 class="fdz-title fdz-m-0">
                    ${task.name}
                </h3>
            </div>
            <span class="fdz-modal-close">&#x2715;</span>
        </div>
        <form data-task="${task.id}" data-group="${group.id}" data-trail="${trail.id}" class="fdz-form-task fdz-w-100-percent">
            <div class="fdz-modal-body">
                <div class="fdz-modal-body-content">
                    <div class="fdz-modal-body-content fdz-m-top-20">
                        ${_this.mountTaskDescription(task)}
                        <div class="fdz-progress-bar-wrapper fdz-m-top-20">
                            ${_this.mountTaskProgressType(task, trail)}
                        </div>
                        
                        <div class="fdz-trail-learning-skills">
                            <h5 class="fdz-title">
                                Conte seus maiores aprendizados                            </h5>
                            ${_this.mountTaskLearningLabel(task, trail)}
                        </div>
                    </div>
                </div>
            </div>
            <div class="fdz-modal-actions fdz-flex-row fdz-m-top-20">
                <div>
                    <a class="fdz-btn back-btn">
                        Voltar                    </a>
                </div>
                <div>
                    ${_this.mountTaskSaveButton(task, trail)}
                </div>
            </div>
        </form>
    </div>
</div>`);
                const modal = $(`#tasks_modal_${task.id}`);

                modal.find("textarea").summernote({
                    lang: `pt-BR`,
                    tooltip: false,
                    height: 120,
                    disableResizeEditor: false,
                    toolbar: [
                        ['style', ['bold', 'italic', 'underline', 'clear']],
                        ['font', ['strikethrough', 'superscript', 'subscript']],
                        ['fontsize', ['fontsize']],
                        ['color', ['color']],
                        ['para', ['ul', 'ol', 'paragraph']],
                        ['height', ['height']]
                    ],
                    placeholder: "Aprendizados",
                    callbacks: {
                        onPaste: function (e) {
                            const bufferText = ((e.originalEvent || e).clipboardData || window.clipboardData).getData('Text');
                            e.preventDefault();
                            document.execCommand('insertText', false, bufferText);
                        },
                    }
                });

                modal.show();
            }

            mountTaskDescription(task) {
                if (task.description.length > 0) {
                    return `<div class="fdz-trail-description-box custom-scrollbar fdz-m-top-20">
                        ${task.description}
                    </div>`;
                }
                return ``;
            }

            mountTaskProgressType(task, trail) {

                let disabledLabel = trail.status == 100 || trail.is_finished ? 'disabled' : '';

                if (task.type == 1) {
                    return `<h5 class="fdz-title">
                            Progresso                        </h5>
                        <div class="fdz-flex-row fdz-m-bottom-8 fdz-flex-align-center">
                            <select name="status" ${disabledLabel} data-task="${task.id}" data-group="${task.id_group}" class="fdz-select fdz-select-medium task-type">
                                <option value="0" ${task.status == 0 ? "selected" : ""}>Não concluído</option>
                                <option value="100" ${task.status == 100 ? "selected" : ""}>Concluído</option>
                            </select>
                        </div>`;
                } else {
                    return `<h5 class="fdz-title">
                            Progresso                        </h5>
                        <div class="fdz-flex-row fdz-m-bottom-8 fdz-flex-align-center">
                            <input name="status" ${disabledLabel} type="number" step="1" min="0" max="100" maxlength="3" data-task="${task.id}" value="${task.status}" class="fdz-input task-status"/><h5 class="fdz-title fdz-m-0 fdz-m-left-8">%</h5>
                        </div>
                        <div class="fdz-progress-bar">
                            <div data-task="${task.id}" class="fdz-progress blue" style="width: ${task.status}%;"></div>
                        </div>`;
                }

            }

            mountTaskLearningLabel(task, trail) {

                let disabledLabel = trail.status == 100 || trail.is_finished ? 'disabled' : '';
                return `<textarea name="learning" ${disabledLabel} class="fdz-input fdz-textarea fdz-height-150">${task.learning ?? ''}</textarea> `;

            }

            mountTaskSaveButton(task, trail) {

                if (trail.status != 100 && !trail.is_finished) {
                    return `<button class="fdz-btn fdz-btn-primary">
                            ${task.type == 1 ? 'Salvar' : 'Atualizar Progresso'}
                        </button>`
                }
                return ``;

            }

            mountTaskStep(step) {
                const stepCorrect = step + 1;
                let stepLabel = `${stepCorrect}`;
                if (step < 10) {
                    stepLabel = `0${stepCorrect}`;
                }
                return stepLabel;
            }

            async getTrailDetails(trailID, onSuccess, onError) {
                return $.get(`/trilhas/232683/trilha/${trailID}`)
                    .done((result) => {
                        onSuccess(result);
                    })
                    .fail(onError);
            }

            async sendFinishTrail() {
                return $.post(`/trilhas/232683/trilha/${this.id}/finalizar`);
            }
        }

    </script>
