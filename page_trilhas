    <script>
        var decodeString = (function () {
            var doc = document.implementation.createHTMLDocument("");
            var element = doc.createElement('div');

            function getText(str) {

                if (!str)
                    return null

                element.innerHTML = str;
                str = element.textContent;
                element.textContent = '';
                return str;
            }

            function decodeHTMLString(str) {
                if (!str)
                    return null

                if (str && typeof str === 'string') {
                    var x = getText(str);
                    while (str !== x) {
                        str = x;
                        x = getText(x);
                    }
                    return x;
                }
            }
            return decodeHTMLString;
        })();
    </script>
    <script>
        document.querySelectorAll(".fdz-list-item-sort").forEach((el) => {
            el.addEventListener("click", (e) => {
                e.preventDefault();
                e.stopPropagation();
                document.querySelectorAll(".fdz-list-item-sort").forEach((element) => {
                    element.parentElement.classList.remove("active");
                })
                e.currentTarget.parentElement.classList.add("active");
            });
        });
    </script>
